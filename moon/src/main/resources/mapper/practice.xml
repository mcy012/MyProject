<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="practice.dao.practiceDao">

    <select id="practiceInfo" parameterType="java.util.Map" resultType="java.util.Map">
        SELECT * FROM practice
    </select>

    <insert id="practiceInsert" parameterType="java.util.Map">
        INSERT INTO practice VALUES (null, #{practiceTitle}, #{practiceContent})
    </insert>

</mapper>